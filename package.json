    "cdk:destroy": "cdk destroy --force"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "aws-cdk": "^2.177.0",
    "eslint": "^9.19.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.23.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.175.1",
    "cdk-nag": "^2.35.10",
    "constructs": "^10.0.0"
  }
}
