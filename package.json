{
  "name": "agents-build-kit",
  "version": "0.1.0",
  "bin": {
    "agents-build-kit": "bin/agents-build-kit.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "cdk": "cdk",
